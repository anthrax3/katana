  "scripts": {
    "start": "npm run build && npm run prod",
    "prod": "NODE_ENV=production node --harmony ./server",
    "dev": "NODE_ENV=development node --harmony ./server",
    "build": "rm -rf public/dist && webpack --progress --color --config webpack.config.prod.js",
    "test": "node ./node_modules/babel-tape-runner/bin/babel-tape-runner ./app/__tests__/*.js",
    "debug": "devtool ./server",
    "lint": "eslint --ext=js --ext=jsx ./app",
    "update": "updtr"
  },
